{
  "packages": {
    ".": {
      "release-type": "node",
      "package-name": "foundryvtt-dnd5e-types",
      "changelog-path": "CHANGELOG.md",
      "bump-minor-pre-major": true,
      "bump-patch-for-minor-pre-major": true,
      "draft": false,
      "prerelease": false,
      "extra-files": [
        {
          "type": "json",
          "path": "version.json",
          "jsonpath": "$.dnd5eVersion"
        }
      ]
    }
  },
  "plugins": ["node-workspace"]
}
